{
  "name": "nestjs-e2e-mock",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf ./yarn.lock && shx rm -rf ./package-lock.json && shx rm -rf ./dist && shx rm -rf ./node_modules",
    "build": "shx rm -rf ./dist && ttsc",
    "start": "cross-env NODE_ENV=development node --inspect dist/main/main.js",
    "test:functional": "./node_modules/.bin/jest --config ./jest.config.js --runInBand --detectOpenHandles --forceExit --testPathPattern test/functional",
    "format": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/eslint ./src --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.7 ",
    "@types/sinon": "^7.0.11",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "ttypescript": "^ 1.5.7",
    "typemoq": "^2.1.0",
    "typescript": "^3.4.4",
    "typescript-transform-paths": "^1.1.4"
  },
  "dependencies": {
    "@nestjs/common": "6.3.1 ",
    "@nestjs/core": "6.3.1 ",
    "@nestjs/platform-express": "6.3.1 ",
    "@nestjs/testing": "6.3.1 ",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "reflect-metadata": "^0.1.13"
  }
}
